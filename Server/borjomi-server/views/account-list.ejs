<!DOCTYPE html>
<html>
  <head>
    <title>Accounts page</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <p>[ <a href="/account/add">Add new account</a> ]</p>
    <h3>Accounts</h3>
    <% if (accounts.length) { %>
        <% accounts.forEach(function(val) { %>
            <div><%= val %></div>
        <% }) %>
    <% } else { %>
        <div>List is empty</div>
    <% } %>
  </body>
</html>
